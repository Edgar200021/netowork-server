{
  "name": "netowork-server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx ./src/main.ts",
    "start:watch": "tsx watch ./src/main.ts",
    "start:prod": "NODE_ENV=production node ./dist/main.js",
    "start:test": "NODE_ENV=test tsx watch ./src/main.ts",
    "build": "tsc",
    "test": "NODE_ENV=test vitest",
    "test:watch": "NODE_ENV=test vitest --watch",
    "biome": "biome check",
    "kysely:codegen": "kysely-codegen --out-file ./src/storage/db.d.ts --camel-case --dialect=postgres",
    "migration:create": "npx kysely migrate:make",
    "migration:run": "npx kysely migrate:latest",
    "migration:rollback": "npx kysely migrate:rollback --all",
    "migration:list": "npx kysely migrate:list",
    "migration:up": "npx kysely migrate:up",
    "migration:down": "npx kysely migrate:down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.21.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "kysely-codegen": "^0.18.2",
    "kysely-ctl": "^0.12.1",
    "pino-pretty": "^13.0.0",
    "prettier": "3.5.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.2",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "@vinejs/vine": "^3.0.0",
    "argon2": "^0.41.1",
    "cloudinary": "^2.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
